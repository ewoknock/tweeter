<div class="feed">
    <%= form_with model: Tweet.new, url: tweets_path, local: true do |f| %>
        <%= f.text_field :body %>
        <%= f.submit %>
    <% end %>

    <ul>
        <% Tweet.find_each do |tweet| %>
            <li><%= tweet.body %></li>
        <% end %>
    </ul>
</div>

<div class="sidebar">
    Sidebar
</div>



